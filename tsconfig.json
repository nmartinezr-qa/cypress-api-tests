{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "outDir": "cypress/dist",
      "types": ["cypress", "mocha"]
    },
    "include": [
      "cypress/**/*.ts"
    ]
  }